{
  "presets": [
    "@babel/env",
    "@babel/typescript",
    "@babel/react"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "@rx-react": "./src"
      }
    }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-transform-runtime", { "regenerator": true }],
    "@babel/plugin-proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}